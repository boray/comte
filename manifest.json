{
    "name": "comte",
    "description": "vocabulary to notion",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
       "scripts": ["background.js"]
   },

    "permissions": [
        "https://api.notion.com/v1/pages",
        "activeTab",
        "storage",
        "contextMenus"
    ],
    "options_ui": {
        "page": "options/options.html",
        "browser_style": true
    },
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128":"images/logo128.png"
    }
}
